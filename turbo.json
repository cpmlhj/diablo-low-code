{
  "$schema": "https://turbo.build/schema.json",
  "pipeline": {
     "build": {
       "dependsOn": ["^build"],
       "outputs": [".next/**", "dist/**"]
     },
     "test": {
       "dependsOn": ["build"],
       "inputs": ["test/**/*.ts", "test/**/*.tsx"]
     },
     "lint": {
       "outputs": []
     },
     "dev": {
        "cache": false
     }
   }
}